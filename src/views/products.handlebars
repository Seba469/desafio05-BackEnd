<div>
    <h1>Productos</h1>

  <article>
    <table>
        <tr>
            <th>Titulo</th>
            <th>Descripcion</th>
            <th>Precio</th>
            <th>Thumnail</th>
            <th>Codigo</th>
            <th>Stock</th>
        </tr> 
        {{#each products}}
            <tr>
                <td>{{this.title}}</td>
                <td>{{this.description}}</td>
                <td>{{this.price}}</td>
                <td>{{this.thumbnail}}</td>
                <td>{{this.code}}</td>
                <td>{{this.stock}}</td>
            </tr>
        {{/each}}
    </table>
</article>  
</div>

<div id="containerForm">
    <form id="form">
        <input id="inputTitle" type="text" placeholder="Titulo" required>
        <input id="inputDescription" type="text" placeholder="Descripcion" required>
        <input id="inputPrice" type="number" placeholder="Precio" required>
        <input id="inputThumbnail" type="text" placeholder="Link foto" required>
        <input id="inputCode" type="text" placeholder="Codigo" required>
        <input id="inputStock" type="number" placeholder="Stock" required>
        <button id="addButton" type="submit">Agregar</button>
    </form>
</div>

<script src="/js/index.js" type="text/javascript"></script>


